{
  "paragraph": {
    "content": "inline*",
    "group": "block",
    "inline": false,
    "attrs": {
      "textAlign": {
        "default": null,
        "allowedValues": ["left", "center", "right", "justify"]
      }
    },
    "htmlTag": "p"
  },
  "doc": {
    "content": "(row | paragraph | heading | image | chart | bulletList | orderedList)+",
    "group": null,
    "inline": false,
    "attrs": {}
  },
  "heading": {
    "content": "inline*",
    "group": "block",
    "inline": false,
    "attrs": {
      "textAlign": {
        "default": null,
        "allowedValues": ["left", "center", "right", "justify"]
      },
      "level": {
        "default": 1,
        "allowedValues": [1, 2, 3, 4, 5, 6]
      }
    },
    "htmlTag": "h"
  },
  "image": {
    "group": "block",
    "attrs": {
      "src": {
        "default": null
      },
      "alt": {
        "default": null
      },
      "layout": {
        "default": "default",
        "allowedValues": [
          "default",
          "full-top",
          "full-bottom",
          "full-left",
          "full-right"
        ]
      },
      "size": {
        "default": null,
        "description": "When the layout value is not default, size attribute defines the width or height of image"
      }
    },
    "htmlTag": "img"
  },
  "chart": {
    "group": "block",
    "attrs": {
      "chartType": {
        "default": "bar",
        "allowedValues": ["line", "bar", "pie"]
      },
      "data": {
        "default": [
          { "label": "Q1", "value": 32 },
          { "label": "Q2", "value": 48 },
          { "label": "Q3", "value": 27 },
          { "label": "Q4", "value": 58 }
        ],
        "description": "Array of { label, value } pairs for the chart data."
      }
    },
    "htmlTag": "chart"
  },
  "column": {
    "content": "block+",
    "group": "block",
    "attrs": {},
    "htmlTag": "column"
  },
  "row": {
    "content": "column+",
    "group": "block",
    "attrs": {
      "columnWidths": {
        "default": [],
        "description": "defines fractional width of columns for grid layout. default width for each column is 1. accumulative widths of all columns should be the same as number of columns."
      }
    },
    "htmlTag": "row"
  },
  "bulletList": {
    "content": "listItem+",
    "group": "block list",
    "attrs": {},
    "htmlTag": "ul"
  },
  "orderedList": {
    "content": "listItem+",
    "group": "block list",
    "htmlTag": "ol"
  },
  "listItem": {
    "content": "paragraph block*",
    "attrs": {},
    "htmlTag": "li"
  }
}
